            "key": "offers_sila_vajebaat",
            "enable": true,
            "options": [
              "Yes",
              "No"
            ]
          },
          {
            "type": "radio",
            "label": "Have you latest attended misaq majlis to Dai-al-Zamaan TUS?",
            "key": "attended_misaq_majlis",
            "enable": true,
            "options": [
              "Yes",
              "No"
            ]
          },
          {
            "type": "radio",
            "label": "Have you come to contact with any moharramaat â€“ (prohibited activities) - Alcoholic substances/drugs/cigarette/Riba/ in your life?",
            "key": "moharramaat_survey",
            "enable": true,
            "conditional_value": "Is He/She still involved?",
            "on_condition": "Yes",
            "condition_options": [
              "Yes",
              "No"
            ],
            "options": [
              "Yes",
              "No"
            ]
          },
          {
            "type": "dropdown",
            "label": "Shaadi?",
            "key": "shaadi_status",
            "enable": true,
            "itemsKey": "shaadiOptions",
            "dropdownLabel": "Select Spouse",
            "dropdownKey": "spouse_name",
            "itemsKey2": "familyMembers",
            "showDropdownIf": 0,
            "itsFieldKey": "spouse_name_its"
          },
          {
            "type": "dropdown",
            "label": "Does he/she attend all 9 days of Ashara Mubarakah punctually?",
            "key": "ashara_attendance",
            "enable": true,
            "itemsKey": "asharaAttendanceOptions"
          },
          {
            "type": "radio",
            "label": "Has he/she done ziyarat of Raudat Tahera?",
            "key": "ziarat_raudat_tahera",
            "enable": true,
            "conditional_value": "Both Moula?",
            "on_condition": "Yes",
            "condition_options": [
              "Yes",
              "No"
            ],
            "options": [
              "Yes",
              "No"
            ]
          },
          {
            "type": "dropdown",
            "label": "Does he/she pray Namaz daily?",
            "key": "namaz_daily",
            "enable": true,
            "itemsKey": "namazDailyOptions"
